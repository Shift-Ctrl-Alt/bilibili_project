<?xml version="1.0" encoding="utf-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="com.oymn.bilibili.dao.AuthRoleDao">

    <select id="getRoleByCode" resultType="com.oymn.bilibili.domain.auth.AuthRole">
        select
            *
        from
            t_auth_role
        where
            code = #{code}
    </select>
</mapper>
